    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "sourceMap": true,
    "removeComments": true,
    "baseUrl": "./src/frontend/scripts",
    "paths": {
      "@entities/*": ["Entities/*"],
      "@modules/*": ["Modules/*"],
      "@tools/*": ["Tools/*"]
